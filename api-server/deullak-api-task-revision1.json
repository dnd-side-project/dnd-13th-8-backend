{
    "family": "api-server",
    "networkMode": "awsvpc",
    "requiresCompatibilities": ["EC2"],
    "cpu": "512",
    "memory": "1024",
    "executionRoleArn": "arn:aws:iam::814022331893:role/ecsTaskExecutionRole",
    "containerDefinitions": [
        {
            "name": "api-server",
            "image": "814022331893.dkr.ecr.ap-northeast-2.amazonaws.com/dulakreposiotry",
            "cpu": 0,
            "portMappings": [
                {
                    "name": "api-server-80-tcp",
                    "containerPort": 80,
                    "hostPort": 80,
                    "protocol": "tcp",
                    "appProtocol": "http"
                },
                {
                    "name": "api-server-443-tcp",
                    "containerPort": 443,
                    "hostPort": 443,
                    "protocol": "tcp",
                    "appProtocol": "http"
                }
            ],
            "essential": true,
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/api-server",
                    "awslogs-create-group": "true",
                    "awslogs-region": "ap-northeast-2",
                    "awslogs-stream-prefix": "ecs"
                }
            }
        }
    ]
}
