{
  "family": "chat-server-ec2",
  "networkMode": "bridge",
  "requiresCompatibilities": ["EC2"],
  "cpu": "256",
  "memory": "512",
  "taskRoleArn": "arn:aws:iam::814022331893:role/ecsTaskExecutionRole",
  "executionRoleArn": "arn:aws:iam::814022331893:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "chat-container",
      "image": "814022331893.dkr.ecr.ap-northeast-2.amazonaws.com/dulakreposiotry:chat-latest",
      "essential": true,
      "portMappings": [
        { "containerPort": 8082, "hostPort": 8082, "protocol": "tcp" }
      ],
      "environment": [],
      "mountPoints": [],
      "volumesFrom": [],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/chat-server",
          "awslogs-create-group": "true",
          "awslogs-region": "ap-northeast-2",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ]
}