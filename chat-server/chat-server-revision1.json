{
    "family": "chat-server",
    "networkMode": "awsvpc",
    "requiresCompatibilities": ["EC2"],
    "cpu": "512",
    "memory": "1024",
    "executionRoleArn": "arn:aws:iam::814022331893:role/ecsTaskExecutionRole",
    "containerDefinitions": [
        {
            "name": "chat-server",
            "image": "814022331893.dkr.ecr.ap-northeast-2.amazonaws.com/dulakreposiotry",
            "cpu": 0,
            "essential": true,
            "portMappings": [
                {
                    "name": "chat-server-80-tcp",
                    "containerPort": 80,
                    "hostPort": 80,
                    "protocol": "tcp",
                    "appProtocol": "http"
                }
            ],
            "environment": [],
            "environmentFiles": [],
            "mountPoints": [],
            "volumesFrom": [],
            "ulimits": [],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/chat-server",
                    "awslogs-create-group": "true",
                    "awslogs-region": "ap-northeast-2",
                    "awslogs-stream-prefix": "ecs"
                },
                "secretOptions": []
            },
            "systemControls": []
        }
    ],
    "runtimePlatform": {
        "cpuArchitecture": "X86_64",
        "operatingSystemFamily": "LINUX"
    },
    "tags": []
}
